{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "React Library",
	"extends": "./base.json",
	"compilerOptions": {
		"jsx": "react-jsx",
		"lib": ["ES2015", "ES2016", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"target": "es6",
		"plugins": [{ "name": "typescript-plugin-css-modules" }]
	}
}
