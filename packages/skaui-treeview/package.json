{
	"name": "@skaui/treeview",
	"version": "1.0.0",
	"description": "SKA UI - TreeView",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/types/index.d.ts",
	"files": [
		"./dist"
	],
	"sideEffects": false,
	"scripts": {
		"build": "rm -rf dist && pnpm run build:full && pnpm run build:min && pnpm run build:types",
		"build:full": "tsup index.ts --format esm,cjs --external react --sourcemap inline --treeshake",
		"build:min": "tsup index.ts --format esm -d dist/min --external react --sourcemap inline --treeshake --minify",
		"build:types": "tsup index.ts --dts-only -d dist/types",
		"dev": "tsup index.ts --format esm,cjs --watch --dts --external react",
		"lint": "TIMING=1 eslint src/**/*.{ts,tsx,js,jsx}* --fix",
		"clean": "rm -rf .turbo && rm -rf dist && rm -rf node_modules"
	},
	"keywords": [],
	"author": "Nam Hyuck Kim (skagur@gmail.com)",
	"license": "ISC",
	"dependencies": {
		"framer-motion": "^6.5.1",
		"react-transition-group": "^4.4.2"
	},
	"devDependencies": {
		"@skaui/utils": "workspace:*",
		"autoprefixer": "^10.4.7",
		"clsx": "^1.2.1",
		"eslint-config-skaui": "workspace:*",
		"postcss": "^8.4.14",
		"react": "^18.2.0",
		"react-aria": "^3.17.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.4.0",
		"tailwindcss": "^3.1.6",
		"tsconfig": "workspace:*",
		"tsup": "^6.2.0",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"react": "^17.2.0",
		"react-dom": "^17.2.0"
	}
}
